{
    "name": "ardiakov/string-stack-calc",
    "description": "Calculator bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Diakov Artem",
            "email": "adiakov.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/var-dumper": "~3.4|~4.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ardiakov\\FirstBundle\\": ""
        },
        "exclude-from-classmap": [
            "vendor/phpunit"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ardiakov\\MathTransformationBundle\\Tests\\": "tests"
        }
    }
}
